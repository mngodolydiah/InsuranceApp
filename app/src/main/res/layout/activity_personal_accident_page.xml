<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingEnd="@dimen/fab_margin"
    android:paddingStart="@dimen/fab_margin"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="mng.r.lydia.insuranceapp.PersonalAccidentPage">

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="32dp"
            android:text="Date of Birth" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26dp"
            android:layout_marginStart="26dp"
            android:ems="10"
            android:inputType="date"
            android:layout_alignBaseline="@+id/textView20"
            android:layout_alignBottom="@+id/textView20"
            android:layout_toRightOf="@+id/textView20"
            android:layout_toEndOf="@+id/textView20" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/editText"
            android:layout_alignStart="@+id/editText"
            android:layout_below="@+id/editText"
            android:layout_marginTop="15dp"
            app:srcCompat="@drawable/ic_menu_manage" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Select"
            android:layout_alignBaseline="@+id/button2"
            android:layout_alignBottom="@+id/button2"
            android:layout_toRightOf="@+id/button2"
            android:layout_toEndOf="@+id/button2" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Upload"
            android:layout_below="@+id/imageView2"
            android:layout_alignLeft="@+id/editText"
            android:layout_alignStart="@+id/editText"
            android:layout_marginTop="16dp" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="48dp"
            android:text="Gender"
            android:layout_below="@+id/button2"
            android:layout_toLeftOf="@+id/editText"
            android:layout_alignLeft="@+id/textView20"
            android:layout_alignStart="@+id/textView20" />

        <RadioButton
            android:id="@+id/radioButton14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Male"
            android:layout_alignBaseline="@+id/textView21"
            android:layout_alignBottom="@+id/textView21"
            android:layout_alignLeft="@+id/button2"
            android:layout_alignStart="@+id/button2" />

        <RadioButton
            android:id="@+id/radioButton15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Female"
            android:layout_alignBaseline="@+id/radioButton14"
            android:layout_alignBottom="@+id/radioButton14"
            android:layout_alignRight="@+id/editText"
            android:layout_alignEnd="@+id/editText" />

        <TextView
            android:id="@+id/textView46"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Period"
            android:layout_below="@+id/radioButton15"
            android:layout_toLeftOf="@+id/editText"
            android:layout_alignLeft="@+id/textView21"
            android:layout_alignStart="@+id/textView21" />
        <RadioButton
            android:id="@+id/radioButton16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Annual"
            android:layout_alignBaseline="@+id/textView46"
            android:layout_alignBottom="@+id/textView46"
            android:layout_alignLeft="@+id/button2"
            android:layout_alignStart="@+id/button2"
            android:layout_below="@+id/radioButton14"/>

        <RadioButton
            android:id="@+id/radioButton17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Semi-annual"
            android:layout_alignBaseline="@+id/radioButton16"
            android:layout_alignBottom="@+id/radioButton16"
            android:layout_alignRight="@+id/editText"
            android:layout_alignEnd="@+id/editText" />

        <TextView
            android:id="@+id/textView47"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Height"
            android:layout_alignBaseline="@+id/editText35"
            android:layout_alignBottom="@+id/editText35"
            android:layout_alignLeft="@+id/textView46"
            android:layout_alignStart="@+id/textView46" />

    <EditText
        android:id="@+id/editText35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/radioButton16"
        android:layout_alignStart="@+id/radioButton16"
        android:layout_below="@+id/radioButton16"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView47"
        android:layout_alignStart="@+id/textView47"
        android:layout_below="@+id/editText35"
        android:layout_marginTop="17dp"
        android:text="TextView" />

    <EditText
        android:id="@+id/editText36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/editText35"
        android:layout_alignStart="@+id/editText35"
        android:layout_below="@+id/editText35"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView48"
        android:layout_alignStart="@+id/textView48"
        android:layout_below="@+id/editText36"
        android:layout_marginTop="24dp"
        android:text="Chose Limit of Cover required either:"
        android:layout_alignRight="@+id/editText36"
        android:layout_alignEnd="@+id/editText36" />

    <RadioButton
        android:id="@+id/radioButton18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView49"
        android:layout_alignStart="@+id/textView49"
        android:layout_below="@+id/textView49"
        android:layout_marginTop="18dp"
        android:text="DIAMOND" />

    <RadioButton
        android:id="@+id/radioButton19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PLATINUM"
        android:layout_alignBaseline="@+id/radioButton18"
        android:layout_alignBottom="@+id/radioButton18"
        android:layout_toRightOf="@+id/radioButton18"
        android:layout_toEndOf="@+id/radioButton18" />

    <RadioButton
        android:id="@+id/radioButton20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/radioButton19"
        android:layout_toEndOf="@+id/radioButton19"
        android:layout_toRightOf="@+id/radioButton19"
        android:text="GOLD" />

    <RadioButton
        android:id="@+id/radioButton21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/radioButton18"
        android:layout_alignStart="@+id/radioButton18"
        android:layout_below="@+id/radioButton18"
        android:text="SILVER" />

    <RadioButton
        android:id="@+id/radioButton22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/radioButton19"
        android:layout_toEndOf="@+id/radioButton18"
        android:layout_toRightOf="@+id/radioButton18"
        android:text="BRONZE" />

    <RadioButton
        android:id="@+id/radioButton23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FEDHA"
        android:layout_below="@+id/radioButton20"
        android:layout_toRightOf="@+id/radioButton22"
        android:layout_toEndOf="@+id/radioButton22" />

    <TextView
        android:id="@+id/textView50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Complete Medical History Questionnaire "
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/radioButton21"
        android:layout_alignStart="@+id/radioButton21"
        android:layout_marginBottom="10dp"
        android:layout_alignRight="@+id/textView49"
        android:layout_alignEnd="@+id/textView49" />
    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:layout_below="@+id/textView50"
        android:layout_alignLeft="@+id/textView50"
        android:layout_alignStart="@+id/textView50" />

    <EditText
        android:id="@+id/editText52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/radioButton16"
        android:layout_alignStart="@+id/radioButton16"
        android:layout_below="@+id/textView50"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

</RelativeLayout>
</ScrollView>